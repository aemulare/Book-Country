@model IEnumerable<BookCountry.Models.Book>

<h1>This page is to show Books</h1>
<br/>
<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Edition</th>
        <th>Published on</th>
        <th>Publisher Id</th>
        <th>Language Id</th>
        <th>Binding format Id</th>
        <th>ISBN13</th>
        <th>Dewey code</th>
        <th>Price, $</th>
        <th>Quantity</th>
    </tr>
    @foreach(var book in Model)
    {
        <tr>
            <td>@book.Id</td>
            <td>@book.Title</td>
            <td>@book.Edition</td>
            <td>@book.PublishedOn.ToString("yyyy-MM-dd")</td>
            <td>@book.Publisher?.Name</td>
            <td>@book.LanguageId</td>
            <td>@book.FormatId</td>
            <td>@book.Isbn</td>
            <td>@book.DeweyCode</td>
            <td>@book.Price</td>
            <td>@book.Quantity</td>
        </tr>
    }
</table>
